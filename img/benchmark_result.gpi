set terminal postscript eps color enhanced size 12cm, 8cm font ",13"

set output "benchmark_result.eps"

set datafile separator ","

set boxwidth 0.5 relative

set grid
set yrange [0:*]

set ylabel "Speedup"

set style line 1 linecolor rgb "#0060ad"
set style line 2 linecolor rgb "#dd181f"

unset key

plot "benchmark_result.csv" every ::1::7 u ($4/$3):xtic(1) w boxes \
    ls 1 fs pattern 2

unset output
